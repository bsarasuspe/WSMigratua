            "author": "mafalda001@ikasle.ehu.eus",
            "itemBody": {
                "p": "\u00bfA qu\u00e9 nos referimos con desarrollo sostenible?"
            },
            "correctResponse": {
                "response": "Al desarrollo que satisface las necesidades del presente sin comprometer las necesidades del futuro."
            },
            "incorrectResponses": {
                "response": [
                    "A que la sociedad crezca lo m\u00e1ximo posible.",
                    "\u00danicamente a los 17 objetivos impulsados por las Naciones Unidas.",
                    "A reutilizar los productos que consumimos."
                ]
            }
        },
